@model IEnumerable<aplication20_07_2019.Models.Marché>

@{
    ViewBag.Title = "MarcheHistorique";
    Layout = "~/Views/Shared/_LayoutPrestataire.cshtml";
}

<h2>MarcheHistorique</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.matricule)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Projet.libelle_projet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.montant_total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date_fin)
        </th>

        <th>
            le responsable marche
        </th>




        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.matricule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Projet.libelle_projet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.montant_total)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date_fin)
            </td>
            

            <td>
                @Html.DisplayFor(modelItem => item.Responsable_marché)
            </td>
            


            <td>

                @Html.ActionLink("Details", "Details", new { id_marche = item.id_marché }) |
                @Html.ActionLink("Refuser", "Refuser", new { id_marche = item.id_marché })
            </td>
        </tr>
    }

</table>

